# Right-aligned prompt
{{ if not .flags.ephemeral -}}
format = """$status$cmd_duration$battery$username$hostname$directory$shell$character"""
right_format = '$all'
{{- end }}

[aws]
symbol = ' '
format = '\[[$symbol($profile)]($style)\]'
expiration_symbol = '[](bold red)'
{{- if .flags.pokemon }}
disabled = false
{{- else }}
disabled = true
{{- end }}

[azure]
disabled = true
format = '\[[$symbol($subscription)]($style)\]'
symbol = 'ﴃ '
style = 'blue bold'

[battery]
format = '\[[$symbol$percentage]($style)\] '
empty_symbol = '󰂃 '
unknown_symbol = '󰂑 '
discharging_symbol = '󱊡 '
charging_symbol = '󰂄 '
full_symbol = '󰁹 '

[[battery.display]]
threshold = 10
style = 'bold red'

[[battery.display]]
threshold = 40
style = 'bold yellow'

[character]
error_symbol = '[](bold red)'

[cmake]
format = '\[[$symbol($version)]($style)\]'

[cmd_duration]
format = '\[[󰔚 $duration]($style)\] '

[cobol]
format = '\[[$symbol($version)]($style)\]'

[conda]
symbol = ' '
format = '\[[$symbol$environment]($style)\]'

[container]
disabled = false
symbol = ''
format = '[$symbol \[$name\]]($style) '

[crystal]
format = '\[[$symbol($version)]($style)\]'

[dart]
symbol = ' '
format = '\[[$symbol($version)]($style)\]'

[deno]
format = '\[[$symbol($version)]($style)\]'

[directory]
read_only = ' '
home_symbol = ' '
style = 'bold 039'

[docker_context]
symbol = ' '
format = '\[[$symbol$context]($style)\]'

[dotnet]
format = '\[[$symbol($version)(🎯 $tfm)]($style)\]'

[elixir]
symbol = ' '
format = '\[[$symbol($version \(OTP $otp_version\))]($style)\]'

[elm]
symbol = ' '
format = '\[[$symbol($version)]($style)\]'

[erlang]
format = '\[[$symbol($version)]($style)\]'

[gcloud]
format = '\[[$symbol$account(@$domain)(\($region\))]($style)\]'

[git_branch]
symbol = ' '
format = '\[[$symbol$branch]($style)\]'
style = 'bold 135'

[git_commit]
format = '\[[󰜛 $hash$tag]($style)\]'
commit_hash_length = 8
tag_symbol = ' / 󰓼 '
disabled = false
tag_disabled = false
only_detached = true

[git_metrics]
disabled = true
format = '\[([+$added]($added_style))([-$deleted]($deleted_style))\]'

[git_state]
format = '\[[$state( \([$progress_current of $progress_total](white))\)]($style)\]'
style = 'bold 199'
merge = ' '
rebase = ' '
revert = ' '
cherry_pick = ' '
bisect = ' '
am = '󰶌 '
am_or_rebase = ' '

[git_status]
format = '\[[$ahead_behind$conflicted$staged$modified$renamed$deleted]($style)\] '
style = 'bold 207'
conflicted = '[](bold red)'
up_to_date = '[󰄬](bold green)'
ahead = '[](bold blue)'
behind = '[](bold blue)'
diverged = '[](bold red)'
untracked = ''
stashed = ''
modified = '[ 󰏫 ](bold yellow)'
staged = '[ 󰝖 ](bold green)'
renamed = '[󰗧 ](bold yellow)'
deleted = '[ ](bold yellow)'

[golang]
symbol = ' '
format = '\[[$symbol($version)]($style)\]'

[helm]
format = '\[[$symbol($version)]($style)\]'

[hg_branch]
symbol = ' '
format = '\[[$symbol$branch]($style)\]'

[hostname]
ssh_symbol = '󰢩 '
format = '\[[$ssh_symbol$hostname]($style)\] '

[java]
symbol = ' '
format = '\[[$symbol($version)]($style)\]'

[julia]
symbol = ' '
format = '\[[$symbol($version)]($style)\]'

[kotlin]
format = '\[[$symbol($version)]($style)\]'

[kubernetes]
format = '\[[$symbol$context( \($namespace\))]($style)\]'
disabled = true

[localip]
ssh_only = true
format = '\[[󰩟 $localipv4]($style)\]'
disabled = true

[lua]
format = '\[[$symbol($version)]($style)\]'

[memory_usage]
